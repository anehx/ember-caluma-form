{{#each field.question.checkboxOptions.edges as |optionEdge i|}}
  {{#let optionEdge.node as |option|}}
    {{#if (gt i 0)}}<br>{{/if}}
    <label>
      <input
        class="uk-checkbox uk-margin-small-right"
        type="checkbox"
        name={{field.id}}
        value={{option.slug}}
        checked={{contains option.slug field.answer.listValue}}
        disabled={{disabled}}
      >
      {{option.label}}
    </label>
  {{/let}}
{{/each}}

